{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "ConnectionStrings": {
        "DefaultConnection": "Data Source=localhost;Database=devicedb;User=root;Password=Password#1;"
    },
    "TcpServerOptions": {
        "Address": "127.0.0.1",
        "Port": 5555
    },
    "MqttClientConfiguration": {
        "Address": "127.0.0.1",
        "Port": 1883,
        "Subscriptions": [ "sensor/+/command" ]
    },
    "RequiredHtppHeaders": {
        "ESP8266": [
            "x-esp8266-sta-mac",
            "x-esp8266-ap-mac",
            "x-esp8266-sdk-version"
        ],
        "ESP32": [
            "x-esp32-sta-mac",
            "x-esp32-ap-mac",
            "x-esp32-sdk-version"
        ]
    },
    "AllowedHosts": "*"
}
